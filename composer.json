{
    "name": "fsi/teryt-database-bundle",
    "type": "symfony-bundle",
    "description": "Teryt database bundle",
    "keywords": ["teryt", "bundle", "symfony", "fsi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "symfony/framework-bundle": "^3.4|^4.0|^5.0",
        "symfony/console": "^3.4|^4.0|^5.0",
        "symfony/dom-crawler": "^3.4|^4.0|^5.0",
        "symfony/css-selector": "^3.4|^4.0|^5.0",
        "doctrine/common": "^2.6",
        "hobnob/xml-stream-reader": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "robrichards/wse-php": "^2.0",
        "robrichards/xmlseclibs": "^3.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "doctrine/doctrine-bundle": "^1.2",
        "doctrine/orm": "^2.4",
        "behat/behat": "^3.3",
        "behat/symfony2-extension": "^2.0",
        "behat/mink-extension": "^2.0",
        "behat/mink-browserkit-driver": "^1.3",
        "phpspec/phpspec": "^5.0",
        "bossa/phpspec2-expect": "^3.0",
        "phpstan/phpstan": "^0.11.4",
        "phpstan/phpstan-doctrine": "^0.11.2"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\TerytDatabaseBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "spec\\": "spec" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
